<?xml version="1.0" ?>
<robot name="quadcopter" xmlns:xacro="http://www.ros.org/wiki/xacro">

<xacro:include filename="$(find quadcopter_description)/urdf/materials.xacro" />
<xacro:include filename="$(find quadcopter_description)/urdf/quadcopter.trans" />
<xacro:include filename="$(find quadcopter_description)/urdf/quadcopter.gazebo" />
<xacro:include filename="$(find quadcopter_description)/urdf/quadmotor.xacro" />
 
  <!-- making cylinder body approximation-->
  <!-- Inertia constants {-->
  <xacro:property name="mass" value="0.614" /> <!-- [kg] -->
  <xacro:property name="inertia_body_radius" value="0.09" /> <!-- [m] -->
  <xacro:property name="inertia_body_height" value="0.043" /> <!-- [m] -->
  <!--
  <xacro:property name="body_inertia"
      ixx="${mass * (3 * inertia_body_radius * inertia_body_radius + inertia_body_height * inertia_body_height) / 12}"
      iyy="${mass * (3 * inertia_body_radius * inertia_body_radius + inertia_body_height * inertia_body_height) / 12}"
      izz="${mass * inertia_body_radius * inertia_body_radius / 2}"
      ixy="0" ixz="0" iyz="0"/>
  </xacro:property>

-->
<link name="base_link">
  <inertial>
    <origin xyz="0 0 0.018151764231791677" rpy="0 0 0"/>
    <mass value="${mass}" />  <!-- [kg] -->
    <inertia ixx="${mass * (3 * inertia_body_radius * inertia_body_radius + inertia_body_height * inertia_body_height) / 12}"
    iyy="${mass * (3 * inertia_body_radius * inertia_body_radius + inertia_body_height * inertia_body_height) / 12}"
    izz="${mass * inertia_body_radius * inertia_body_radius / 2}"
    ixy="0" ixz="0" iyz="0"/>
  </inertial>
  <visual>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <geometry>
      <mesh filename="package://quadcopter_description/meshes/base_link.stl" scale="0.001 0.001 0.001"/>
    </geometry>
    <material name="plastic_glossy_grey"/>
  </visual>
  <collision>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <geometry>
      <mesh filename="package://quadcopter_description/meshes/base_link.stl" scale="0.001 0.001 0.001"/>
    </geometry>
  </collision>
</link>

<link name="prop1_1">
  <inertial>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <mass value="0.003"/>
    <inertia ixx="0.0001" ixy="0.0" ixz="0.0" iyy="0.0001" iyz="0.0" izz="0.0001"/>
  </inertial>
  <visual>
    <origin xyz="-0.076368 0.099475 -0.03342" rpy="0 0 0"/>
    <geometry>
      <mesh filename="package://quadcopter_description/meshes/prop1_1.stl" scale="0.001 0.001 0.001"/>
    </geometry>
